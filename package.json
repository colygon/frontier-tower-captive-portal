{
  "name": "frontier-tower-captive-portal",
  "version": "1.0.0",
  "description": "Branded Node.js Captive Portal for Frontier Tower based on unifi-hotspot",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "vercel-build": "npm run build",
    "docker:build": "docker build -t frontier-tower-captive-portal .",
    "docker:run": "docker run -p 4545:4545 frontier-tower-captive-portal"
  },
  "keywords": ["unifi", "captive-portal", "wifi", "hotspot", "typescript"],
  "author": "Frontier Tower",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "node-unifi": "^2.5.1",
    "axios": "^1.6.0",
    "dotenv": "^16.3.1",
    "helmet": "^7.1.0",
    "cors": "^2.8.5",
    "body-parser": "^1.20.2",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@types/cors": "^2.8.17",
    "typescript": "^5.3.2",
    "ts-node": "^10.9.1"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
